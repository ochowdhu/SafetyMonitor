@INPROCEEDINGS{Bloomfield2007,
  author = {Bloomfield, R.E. and Littlewood, Bev and Wright, D.},
  title = {Confidence: Its Role in Dependability Cases for Risk Assessment},
  booktitle = {Dependable Systems and Networks, 2007. DSN '07. 37th Annual IEEE/IFIP
	International Conference on},
  year = {2007},
  pages = {338-346},
  month = {June},
  abstract = {Society is increasingly requiring quantitative assessment of risk
	and associated dependability cases. Informally, a dependability case
	comprises some reasoning, based on assumptions and evidence, that
	supports a dependability claim at a particular level of confidence.
	In this paper we argue that a quantitative assessment of claim confidence
	is necessary for proper assessment of risk. We discuss the way in
	which confidence depends upon uncertainty about the underpinnings
	of the dependability case (truth of assumptions, correctness of reasoning,
	strength of evidence), and propose that probability is the appropriate
	measure of uncertainty. We discuss some of the obstacles to quantitative
	assessment of confidence (issues of composability of subsystem claims;
	of the multi-dimensional, multi-attribute nature of dependability
	claims; of the difficult role played by dependence between different
	kinds of evidence, assumptions, etc). We show that, even in simple
	cases, the confidence in a claim arising from a dependability case
	can be surprisingly low.},
  doi = {10.1109/DSN.2007.29},
  keywords = {risk management;software reliability;claim confidence;dependability
	claim;risk assessment;Application software;Battery powered vehicles;Calculus;Costs;Humans;Measurement
	uncertainty;Probability;Risk management;Software reliability;Software
	safety},
  timestamp = {2014.12.11}
}

